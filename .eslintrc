{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": false,
    "node": true
  },
  "globals": {
    "window": true
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "comma-dangle": 0,
    "no-console": 1,
    "no-multi-spaces": [1, {
      "exceptions": {
        "VariableDeclarator": true
      }
    }],
    "react-native/no-unused-styles": 2,
    "no-return-assign": 0,
    "react/jsx-boolean-value": 0,
    "react/prop-types": 0
  }
}